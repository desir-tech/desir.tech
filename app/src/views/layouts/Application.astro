---
import "@libs/globals.css";

import Header from "@includes/dynamic/Header";
import Footer from "@includes/static/Footer/Footer.astro";
import Meta from "@includes/static/Meta/Meta.astro";
import ContactSection from "@includes/dynamic/ContactSection";
import Container from "@components/static/Container/Container.astro";
import Navigator from "@includes/dynamic/Navigator";

export const defaultProps = {
  header: {}
}
const { layout } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Meta  />
  <Container>
    <Header {...layout?.header ?? defaultProps.header}/>
    <slot />
    <ContactSection />
    <Footer />
    <Navigator />
  </Container>
</html>
